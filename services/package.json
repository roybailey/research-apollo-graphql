{
  "name": "research-graphql-typescripti-services",
  "version": "0.1.0",
  "description": "Micro Service Emulation APIs (in Typescript!)",
  "author": "Roy Bailey",
  "private": true,
  "license": "MIT",
  "scripts": {
    "test": "npx jest --watchAll",
    "start": "npm run build:live",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/apiServices.ts"
  },
  "jest": {
    "verbose": true,
    "rootDir": "src",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "date-fns": "^1.30.1",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "log4js": "^6.3.0",
    "lokijs": "^1.5.11",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.11",
    "@types/faker": "^4.1.12",
    "@types/jest": "^23.3.14",
    "@types/log4js": "^2.3.5",
    "@types/lokijs": "^1.5.3",
    "@types/node": "^10.17.55",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^3.4.9",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "supertest": "^3.4.2",
    "ts-jest": "^27.0.7",
    "ts-node": "^6.2.0",
    "typescript": "^2.9.2"
  }
}
